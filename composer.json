{
    "name": "zhiyicx/thinksns-plus",
    "description": "一个使用 Laravel 并健壮的社交系统",
    "keywords": [
        "thinksns",
        "web",
        "php",
        "laravel",
        "sns",
        "user",
        "think",
        "app"
    ],
    "license": "Apache-2.0",
    "type": "project",
    "repositories": [
        {
            "type": "artifact",
            "url": "resources/repositorie/zips/"
        },
        {
            "type": "path",
            "url": "packages/slimkit-plus-appversion",
            "options": {
                "symlink": true,
                "plus-soft": true
            }
        },
        {
            "type": "path",
            "url": "packages/slimkit-plus-around-amap",
            "options": {
                "symlink": true,
                "plus-soft": true
            }
        },
        {
            "type": "path",
            "url": "packages/slimkit-plus-checkin",
            "options": {
                "symlink": true,
                "plus-soft": true
            }
        },
        {
            "type": "path",
            "url": "packages/slimkit-plus-feed",
            "options": {
                "symlink": true,
                "plus-soft": true
            }
        },
        {
            "type": "path",
            "url": "packages/slimkit-plus-id",
            "options": {
                "symlink": true,
                "plus-soft": true
            }
        },
        {
            "type": "path",
            "url": "packages/slimkit-plus-music",
            "options": {
                "symlink": true,
                "plus-soft": true
            }
        },
        {
            "type": "path",
            "url": "packages/slimkit-plus-news",
            "options": {
                "symlink": true,
                "plus-soft": true
            }
        },
        {
            "type": "path",
            "url": "packages/slimkit-plus-socialite",
            "options": {
                "symlink": true,
                "plus-soft": true
            }
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.3",
        "jpush/jpush": "^3.5",
        "laravel/framework": "5.5.*",
        "overtrue/easy-sms": "^1.0",
        "pingplusplus/pingpp-php": "^2.2",
        "predis/predis": "^1.1",
        "slimkit/plus-appversion": "1.7.0",
        "slimkit/plus-around-amap": "1.7.0",
        "slimkit/plus-checkin": "1.7.0",
        "slimkit/plus-feed": "1.7.0",
        "slimkit/plus-id": "1.7.0",
        "slimkit/plus-music": "1.7.0",
        "slimkit/plus-news": "1.7.0",
        "slimkit/plus-socialite": "1.7.0",
        "symfony/finder": "~3.3",
        "symfony/polyfill-mbstring": "^1.4",
        "symfony/yaml": "^3.3",
        "tymon/jwt-auth": "1.0.0-rc.1"
    },
    "require-dev": {
        "codacy/coverage": "^1.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Zhiyi\\Plus\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zhiyi\\Plus\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.7-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.0"
        }
    }
}
