        "vue": "^2.4.2",
        "vue-router": "^2.7.0",
        "vuex": "^2.3.1",
        "vuex-router-sync": "^4.2.0"
    },
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 5.0.0"
    },
    "browserslist": [
        "Android 2.3",
        "Android >= 4",
        "Chrome >= 20",
        "Firefox >= 24",
        "Explorer >= 8",
        "iOS >= 6",
        "Opera >= 12",
