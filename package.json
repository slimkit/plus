{
    "private": true,
    "license": "Apache-2.0",
    "scripts": {
        "clean-dist": "trash public/js public/css public/images public/fonts public/hot public/mix-manifest.json",
        "dev": "npm run clean-dist && cross-env NODE_ENV=development webpack --progress --hide-modules --colors",
        "watch": "npm run dev -- -w",
        "watch-poll": "npm run dev -- -w --watch-poll",
        "hot": "npm run clean-dist && cross-env NODE_ENV=development webpack-dev-server --inline --hot",
        "dist": "npm run clean-dist && cross-env NODE_ENV=production webpack --display-error-details --progress --colors"
    },
    "devDependencies": {
        "autoprefixer": "^7.1.2",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.5",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^0.11.2",
        "node-sass": "^4.5.3",
        "optimize-css-assets-webpack-plugin": "^3.1.1",
        "sass-loader": "^6.0.6",
        "trash-cli": "^1.4.0",
        "url-loader": "^0.5.9",
        "vue-loader": "^13.0.4",
        "vue-template-compiler": "^2.4.2",
        "webpack": "^3.5.5",
        "webpack-dev-server": "^2.7.1",
        "webpack-laravel-mix-manifest": "^1.0.4"
    },
    "dependencies": {
        "axios": "^0.16.2",
        "bootstrap-sass": "^3.3.7",
