    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "node-sass": "^4.5.2",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "sass-loader": "^6.0.3",
    "trash-cli": "^1.4.0",
    "url-loader": "^0.5.8",
    "vue-loader": "^12.0.3",
    "vue-template-compiler": "^2.3.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-laravel-mix-manifest": "^1.0.3"
  },
